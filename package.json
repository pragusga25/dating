{
  "name": "dealls",
  "module": "index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev ./src/server.ts --watch",
    "build": "tsc",
    "test": "jest --coverage",
    "check": "npx @biomejs/biome check ./src",
    "check:write": "npx @biomejs/biome check --write ./src",
    "lint": "npx @biomejs/biome lint ./src",
    "lint:write": "npx @biomejs/biome lint --write ./src",
    "format": "npx @biomejs/biome format ./src",
    "format:write": "npx @biomejs/biome format --write ./src",
    "prepush": "npm run test",
    "precommit": "npx @biomejs/biome check --write ./src",
    "commit": "cz"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "^1.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.4.0",
    "@types/node-schedule": "^2.1.7",
    "@types/supertest": "^6.0.2",
    "bun-types": "^1.1.24",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.18.0",
    "prisma-dbml-generator": "^0.12.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "node-schedule": "^2.1.1",
    "zod": "^3.23.8",
    "jest-mock-extended": "^3.0.7"
  }
}
